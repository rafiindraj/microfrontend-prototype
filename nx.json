    "ui": { "tags": [] },
    "fordos": { "tags": [] },
    "fordos-e2e": { "tags": [], "implicitDependencies": ["fordos"] },
    "appshell": { "tags": [] },
    "appshell-e2e": { "tags": [], "implicitDependencies": ["appshell"] }
  }
